{
  "compilerOptions": {
    "types": [
      "@types/node",
      "vite/client"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "resolveJsonModule": true,
  },
  "include": [
    "src",
    "global.d.ts"
  ]
}